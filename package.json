{
	"name": "n8n-nodes-fal-ai",
	"version": "1.0.0",
	"description": "n8n community node for fal.ai - access hundreds of AI models for image generation, video generation, text-to-speech, and more",
	"keywords": [
		"n8n-community-node-package",
		"fal.ai",
		"ai",
		"image-generation",
		"video-generation",
		"text-to-speech",
		"speech-to-text",
		"flux",
		"kling",
		"stable-diffusion"
	],
	"license": "MIT",
	"homepage": "https://github.com/fal-ai-community/n8n-nodes-fal-ai",
	"author": {
		"name": "fal.ai",
		"email": "support@fal.ai"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fal-ai-community/n8n-nodes-fal-ai.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "dist/nodes/FalAi/FalAi.node.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/FalAiApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/FalAi/FalAi.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^24.10.2",
		"@typescript-eslint/eslint-plugin": "^8.49.0",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
